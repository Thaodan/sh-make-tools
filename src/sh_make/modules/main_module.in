#!/bin/sh
SHMK_loaded=main_module

run() {
  while [ ! $# = 0 ] ; do
    $1
    shift
  done
}

load(){
  if echo $SHMK_loaded | grep -vq $( basename $1 ) ; then
      case "$1" in
	  */*)
	      . "$1"
	      ;;
	  *)
	      if [ -e $MODULE_PATH/$1 ] ; then
		  . $MODULE_PATH/$1
	      else
		  return 1
	      fi
	      ;;
      esac 
      SHMK_loaded=$SHMK_loaded:$( basename $1 )
  else
      return 1
  fi
}
