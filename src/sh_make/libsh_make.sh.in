pass_chained_vars() {
  if [ -z $( eval echo $target_var ) ] ; then 
    $target_vars=$( echo $1 | cut -d '=' -f1 )
  else
   $target_vars=\$$target_vars:$( echo $1 | cut -d '=' -f1 )
  fi
  unset target_var
}


init_stats