pkgname=sh-make-tools
pkgver=2.0
pkgrel=1
pkgdesc="make replacement written for POSIX compilant shells like dash or bash and some tools around i like shpp(shell preprocessor)"
arch=('any')
#url="http://nadvsh.sourceforge.net/"
license=('GPL2')
depends=('sh' 'coreutils' 'sed' 'libsh')
source=("${pkgname}-${pkgver}.tar.gz")
md5sums=('453e77b33902d245b4ed9837a82dddbd')






package() {
  cd $srcdir/$pkgname-$pkgver
  sh dist/build_app
  mv pkg/* $pkgdir/.
}